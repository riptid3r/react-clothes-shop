    "axios": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "swiper": "^9.2.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.0",
